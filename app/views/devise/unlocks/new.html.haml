= provide(:title, "Re-send unlock instructions")
= provide(:headline, "Re-send unlock instructions")

%p
  = form_for(resource, :as => resource_name, :url => unlock_path(resource_name), :html => { :method => :post }) do |f|
    = render 'devise/shared/error_messages'
    = f.label :email
    = f.email_field :email
    %p= f.submit "Re-send unlock instructions", class: "btn btn-large btn-primary"

= render "devise/shared/links"